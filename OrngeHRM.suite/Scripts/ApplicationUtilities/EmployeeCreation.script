to createNewEmployeeInPMPModule
	"commonUtilities/Core".mouseAction "PIM", "Employee Information", "LHLH", "LH"
	"commonUtilities/Core".mouseAction "Add", "Employee Full Name", "LHLH","TH"
	"commonUtilities/Core".textEntry "First Name", universal data."firstName","TH"
	wait 1
	"commonUtilities/Core".textEntry "Middle Name", universal data."middleName"
	"commonUtilities/Core".textEntry "Last Name", universal data."lastName"
	"commonUtilities/Core".validateElement universal data."firstName", "TH"
	--"commonUtilities/Core".mouseAction "uploadProfileIcon", "File name", "TLQ", "TLQ"
	--"commonUtilities/Core".textEntry "File name", photoPath, "TH"
	--"commonUtilities/Core".mouseAction "Open", "profilePhoto", "HM"
end createNewEmployeeInPMPModule

to enableCreateLoginCredentials
	"commonUtilities/Core".mouseAction "Create Login Details", "Enabled", "LH", "RH",[100,0]
	put "commonUtilities/Core".getRandomName into universal loginUserName
	"commonUtilities/Core".textEntry "Username", universal loginUserName, "LH",[10,50]
	"commonUtilities/Core".textEntry "Password", universal data."loginPassword", "LH",[10,50]
	"commonUtilities/Core".textEntry "Confirm password", universal data."loginPassword", "RH",[10,50]
	"commonUtilities/Core".mouseAction "Save", , "RHRH"
	if ImageFound(text:"Save Password", waitFor:universal meanWaitTime,SearchRectangle:"commonUtilities/screenPart"().SUT.("TH") )
		"commonUtilities/Core".mouseAction "Not now", "", "TH"
	end if
	"commonUtilities/core".validateElement "Personal Details", "TLQ"
end enableCreateLoginCredentials

to fillContactDetails
	"commonUtilities/Core".mouseAction "Contact Details", "Contact Details", "LH", "THTH"
	"commonUtilities/Core".textEntry "Street 1", universal data."Street1", "TH",[10,50]
	"commonUtilities/Core".textEntry "Street 2", universal data."Street2", "TH",[10,50]
	"commonUtilities/Core".textEntry "City", universal data."city", "TH",[10,50]
	"commonUtilities/Core".textEntry "State", universal data."state", "TH",[10,50]
	"commonUtilities/Core".textEntry "Zip", universal data."zip", "TH",[10,50]
	"commonUtilities/Core".selectDropDownElement "Country", , "RH", [10,50]
	"commonUtilities/Core".textEntry "Home", universal data."homeTelephone", "HM",[10,50]
	"commonUtilities/Core".textEntry "Mobile", universal data."mobile", "HM",[10,50]
	"commonUtilities/Core".textEntry "Work", universal data."workTelephone", "RH",[10,50]
	put "commonUtilities/Core".getRandomName into workEmail
	"commonUtilities/Core".textEntry "Work Email", workEmail&"@gmail.com", "HM",[10,50]
	put "commonUtilities/Core".getRandomName into otherEmail
	"commonUtilities/Core".textEntry "Other Email", otherEmail&"@gmail.com", "HM",[10,50]
	CaptureScreen {Name: universal screenshotPath & "ContactDetails" && the hour && the minute }
	wait 2
	"commonUtilities/Core".mouseAction "Save", , "RHRH"
	set the TextStyle to {dpi:144}
	"commonUtilities/Core".validateElement "Success", "BLQ"
	set the TextStyle to empty
	if ImageFound(text:"Save Address", waitFor:universal meanWaitTime,SearchRectangle:"commonUtilities/screenPart"().SUT.("TH") )
		"commonUtilities/Core".mouseAction "Not now", "", "TH"
	end if
end fillContactDetails

