to copyEmployeeID
	"commonUtilities/Core".mouseAction "Personal Details", , "TLQ"
	"commonUtilities/Core".mouseAction "Employee Id", , "TLQ", ,[10,50]
	typetext controlKey, a, c
	set universal employeeId to RemoteClipboard()
	log universal employeeId
end copyEmployeeID

to SearchEmployeeInEmployeeListPage
	"commonUtilities/Core".mouseAction "Employee List", "Employee Information", "TLQ", "TLQ"
	"commonUtilities/Core".textEntry "Employee Id", universal employeeId, ,[10,50]
	"commonUtilities/Core".mouseAction "Search", "(1) Record Found", "RH", "LH"
	"commonUtilities/Core".mouseAction  universal employeeId, "Personal Details", "BLQ", "TLQ"
	"commonUtilities/Core".validateElement  universal employeeId, "TLQ"
end SearchEmployeeInEmployeeListPage
