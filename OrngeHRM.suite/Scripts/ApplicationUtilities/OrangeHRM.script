to createNewEmployeeInPMPModule firstName, middleName, lastName, photoPath
	"commonUtilities/Core".mouseAction "PIM", "Employee Information", "LHLH", "LH"
	"commonUtilities/Core".mouseAction "Add", "Employee Full Name", "LHLH", "LH"
	"commonUtilities/Core".textEntry "First Name", firstName, "TH"
	"commonUtilities/Core".textEntry "Middle Name", middleName, "TH"
	"commonUtilities/Core".textEntry "Last Name", lastName, "TH"
	"commonUtilities/Core".mouseAction "uploadProfileIcon", "File name", "TLQ", "TLQ"
	"commonUtilities/Core".textEntry "File name", photoPath, "TH"
	"commonUtilities/Core".mouseAction "Open", "profilePhoto", "HM"
end createNewEmployeeInPMPModule

to enableCreateLoginCredentials
	"commonUtilities/Core".mouseAction "Create Login Details", "Enabled", "LH", "RH",[100,0]
	put "commonUtilities/Core".getRandomName into global userName
	put "Testing@123" into global password
	"commonUtilities/Core".textEntry "Username", global userName, "LH",[10,50]
	"commonUtilities/Core".textEntry "Password", global password, "LH",[10,50]
	"commonUtilities/Core".textEntry "Confirm password", global password, "RH",[10,50]
	"commonUtilities/Core".mouseAction "Save", "Personal Details", "RHRH", "TLQ"
end enableCreateLoginCredentials

to fillContactDetails
	"commonUtilities/Core".mouseAction "Contact Details", "Contact Details", "LH", "THTH"
	"commonUtilities/Core".textEntry "Street 1", "Street1", "TH",[10,50]
	"commonUtilities/Core".textEntry "Street 2", "Street2", "TH",[10,50]
	"commonUtilities/Core".textEntry "City", "Hyderabad", "TH",[10,50]
	"commonUtilities/Core".textEntry "State", "Telangana", "TH",[10,50]
	"commonUtilities/Core".textEntry "Zip", "500032", "TH",[10,50]
	"commonUtilities/Core".selectDropDownElement "Country", , "RH", [10,50]
	"commonUtilities/Core".textEntry "Home", "1234567890", "HM",[10,50]
	"commonUtilities/Core".textEntry "Mobile", "0987654321", "HM",[10,50]
	"commonUtilities/Core".textEntry "Work", "5432167890", "RH",[10,50]
	"commonUtilities/Core".textEntry "Work Email", "TestingWork@gmail.com", "HM",[10,50]
	"commonUtilities/Core".textEntry "Other Email", "TestingOther@gmail.com", "HM",[10,50]
	CaptureScreen {Name: expectation, increment:true}
	wait 2
	"commonUtilities/Core".mouseAction "Save", "Success", "RHRH", "BLQ"
	if ImageFound(text:"Save Address", waitFor:universal meanWaitTime,SearchRectangle:"commonUtilities/screenPart"().SUT.("TH") )
		"commonUtilities/Core".mouseAction "Not now", "", "TH"
	end if
end fillContactDetails

