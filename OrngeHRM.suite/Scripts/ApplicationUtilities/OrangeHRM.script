to copyEmployeeID
	"commonUtilities/Core".mouseAction "Employee Id", , "TLQ", ,[10,50]
	typetext controlKey, a, c
	set global employeeId to RemoteClipboard()
	log global employeeId
end copyEmployeeID

to SearchEmployeeInEmployeeListPage
	"commonUtilities/Core".mouseAction "Employee List", "Employee Information", "TLQ", "TLQ"
	"commonUtilities/Core".textEntry "Employee Id", global employeeId, "RH",[10,50]
	"commonUtilities/Core".mouseAction "Search", "(1) Record Found", "RH", "LH"
	"commonUtilities/Core".mouseAction  global employeeId, "Employee Informtion", "RH", "TLQ"
	"commonUtilities/Core".validateElement  global employeeId, "TLQ"
end SearchEmployeeInEmployeeListPage
